axios\";\r\nimport { useEffect, useState } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { Alert, Button, Container, Form, FormGroup, Input, Label } from \"reactstrap\";\r\nimport { api } from \"../../../config\";\r\n\r\nexport const EditarCli = (props) => {\r\n\r\n    console.log(props.match.params.id)\r\n\r\n    const [id] = useState(props.match.params.id)\r\n\r\n    const [cliente, setCliente] = useState({\r\n        nome: '',\r\n        endereco: '',\r\n        cidade: '',\r\n        uf: '',\r\n        nascimento: '',\r\n        clienteDesde: '',\r\n    });\r\n\r\n    const valorInput = e => setCliente({\r\n        ...cliente,\r\n        [e.target.name]: e.target.value\r\n    });\r\n\r\n    const [status, setStatus] = useState({\r\n        type: '',\r\n        message: ''\r\n    });\r\n\r\n    const getCliente = async () => {\r\n        await axios.get(api + \"/cliente/\" + id)\r\n            .then((response) => {\r\n                console.log(response.data.cli)\r\n                setCliente(response.data.cli)\r\n            })\r\n            .catch(() => {\r\n                setStatus({\r\n                    type: 'error',\r\n                    message: 'Erro: sem conexão com a API.'\r\n                })\r\n            });\r\n    };\r\n\r\n    const excluirCliente = async () => {\r\n\r\n        const headers = {\r\n            'Content-Type': 'application/json'\r\n        };\r\n\r\n        await axios.delete(api + \"/excluircliente/\" + id, { headers })\r\n            .then((response) => {\r\n                setStatus({\r\n                    type: 'success',\r\n                    message: response.data.message\r\n                });\r\n            })\r\n            .catch(() => {\r\n                setStatus({\r\n                    type: \"error\",\r\n                    message: \"Erro: sem conexão com a API.\"\r\n                })\r\n            })\r\n    }\r\n\r\n    const editCliente = async e => {\r\n        e.preventDefault();\r\n        console.log(\"Editar\")\r\n\r\n        const headers = {\r\n            'Content-Type': 'application/json'\r\n        };\r\n\r\n        await axios.put(api + \"/cliente/\" + id + \"/editar\", cliente, { headers })\r\n            .then((response) => {\r\n                setStatus({\r\n                    type: 'success',\r\n                    message: response.data.message\r\n                });\r\n            })\r\n            .catch(() => {\r\n                setStatus({\r\n                    type: 'error',\r\n                    message: \"Sem conexão com a API.\"\r\n                });\r\n            });\r\n    }\r\n\r\n    useEffect(() => {\r\n        getCliente();\r\n    }, [])\r\n\r\n\r\n    return (\r\n        <div>\r\n            <Container>\r\n                <div className=\"d-flex\">\r\n                    <div>\r\n                        <h1>Editar Cliente</h1>\r\n                    </div>\r\n                    <div className=\"m-auto p-2\">\r\n                        <Link to=\"/listar-cliente\" className=\"btn btn-outline-primary btn-sm\">\r\n                            Cliente\r\n                        </Link>\r\n                    </div>\r\n                </div>\r\n                <hr className=\"m-1\" />\r\n\r\n                {status.type === 'error' ? <Alert color=\"danger\">{status.message}</Alert> : \"\"}\r\n                {status.type === 'success' ? <Alert color=\"success\">{status.message}</Alert> : \"\"}\r\n\r\n                <Form className=\"p-2\" onSubmit={editCliente}>\r\n                    <FormGroup className=\"p2\">\r\n                        <Label>\r\n                            Nome\r\n                        </Label>\r\n                        <Input\r\n                            name=\"nome\"\r\n                            placeholder=\"Nome do Cliente\"\r\n                            type=\"text\"\r\n                            defaultValue={cliente.nome}\r\n                            onChange={valorInput}\r\n                        />\r\n                    </FormGroup>\r\n                    <FormGroup>\r\n                        <Label className=\"p-2\">\r\n                            Endereço\r\n                        </Label>\r\n                        <Input\r\n                            name=\"endereco\"\r\n                            placeholder=\"Endereço do Cliente\"\r\n                            type=\"text\"\r\n                            defaultValue={cliente.endereco}\r\n                            onChange={valorInput}\r\n                        />\r\n                    </FormGroup>\r\n                    <FormGroup className=\"p2\">\r\n                        <Label>\r\n                            Cidade\r\n                        </Label>\r\n                        <Input\r\n                            name=\"cidade\"\r\n                            placeholder=\"Cidade do Cliente\"\r\n                            type=\"text\"\r\n                            defaultValue={cliente.cidade}\r\n                            onChange={valorInput}\r\n                        />\r\n                    </FormGroup>\r\n                    <FormGroup className=\"p2\">\r\n                        <Label>\r\n                            UF\r\n                        </Label>\r\n                        <Input\r\n                            name=\"uf\"\r\n                            placeholder=\"UF\"\r\n                            type=\"text\"\r\n                            defaultValue={cliente.uf}\r\n                            onChange={valorInput}\r\n                        />\r\n                    </FormGroup>\r\n                    <FormGroup className=\"p2\">\r\n                        <Label>\r\n                            Nascimento\r\n                        </Label>\r\n                        <Input\r\n                            name=\"nascimento\"\r\n                            placeholder=\"Nascimento do Cliente\"\r\n                            type=\"date\"\r\n                            defaultValue={cliente.nascimento}\r\n                            onChange={valorInput}\r\n                        />\r\n                    </FormGroup>\r\n                    <FormGroup className=\"p2\">\r\n                        <Label>\r\n                            Cliente Desde\r\n                        </Label>\r\n                        <Input\r\n                            name=\"clienteDesde\"\r\n                            placeholder=\"Cliente Desde\"\r\n                            type=\"date\"\r\n                            defaultValue={cliente.clienteDesde}\r\n                            onChange={valorInput}\r\n                        />\r\n                    </FormGroup>\r\n\r\n\r\n                    <Button type=\"button\" outline color=\"danger\" onClick={excluirCliente}>\r\n                        Excluir\r\n                    </Button>\r\n\r\n                    <Button type=\"submit\" outline color=\"success\">\r\n                        Salvar\r\n                    </Button>\r\n\r\n                </Form>\r\n\r\n            </Container>\r\n        </div>\r\n    );\r\n};","C:\\Users\\joaop\\Wamp\\www\\tiAcademy-React\\src\\views\\Servico\\Editar\\index.js",["100"],"C:\\Users\\joaop\\Wamp\\www\\tiAcademy-React\\src\\views\\Produto\\Listar\\index.js",[],"C:\\Users\\joaop\\Wamp\\www\\tiAcademy-React\\src\\views\\Produto\\Cadastrar\\index.js",[],"C:\\Users\\joaop\\Wamp\\www\\tiAcademy-React\\src\\views\\Compra\\Listar\\index.js",[],"C:\\Users\\joaop\\Wamp\\www\\tiAcademy-React\\src\\views\\Pedido\\Cadastrar\\index.js",[],"C:\\Users\\joaop\\Wamp\\www\\tiAcademy-React\\src\\views\\Compra\\Cadastrar\\index.js",[],"C:\\Users\\joaop\\Wamp\\www\\tiAcademy-React\\src\\views\\Cliente\\Consulta\\index.js",[],"C:\\Users\\joaop\\Wamp\\www\\tiAcademy-React\\src\\views\\Pedido\\CadastrarItem\\index.js",[],"C:\\Users\\joaop\\Wamp\\www\\tiAcademy-React\\src\\components\\Header\\Menu.js",[],"C:\\Users\\joaop\\Wam